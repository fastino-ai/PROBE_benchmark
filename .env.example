# PROBE Environment Variables
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for data generation and OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (optional, for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google/Gemini API Key (optional, for Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Together.ai API Key (optional, for Together.ai models)
TOGETHERAI_API_KEY=your_togetherai_api_key_here

# HuggingFace Token (optional, for persona dataset)
HUGGINGFACE_TOKEN=your_huggingface_token_here
